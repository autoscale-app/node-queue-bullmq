  },
  "devDependencies": {
    "@tsconfig/node16-strictest": "^1.0.4",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "jest": "^29.4.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-standard": "^12.0.2",
    "typescript": "^4.9.4"
  }
}
